<!DOCTYPE html>
<html ng-app="PNGArticlesListing" data-country="png">
<head>
	<title>Loop Reporting App</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<% include partials/stylesheets %>
</head>
<body style="padding-top: 75px;">
<!-- main navigation inclusion from .ejs file -->
<% include partials/mainNavigation %>
<main class="container">
	<div class="row">
		<table class="table">
			<caption>Caption: This table is the data exported from the database.</caption>
			<button id="export-btn" class="btn btn-success btn-block" data-export="export">Export Table</button>
			<thead class="text-uppercase">
				<tr><th>Title</th><th>Author</th><th>Freelance</th><th>Views</th></tr>
			</thead>
			<tbody>
			<% result.map(function(data) { %>
				<tr><td><%= data.title %></td><td><%= data.author %></td><td>NO</td><td><%= data.views %></td></tr>
			<% }); %>
			</tbody>
		</table>
	</div>
</main><hr />
<footer class="container-fluid well bg-default">
	<div class="col-md-4">
		<h1 class="text-uppercase">widget area</h1>
		<p class="text-lowercase">widget area description</p>
	</div>
	<div class="col-md-4">
		<h1 class="text-uppercase">widget area</h1>
		<p class="text-lowercase">widget area description</p>
	</div>
	<div class="col-md-4">
		<h1 class="text-uppercase">widget area</h1>
		<p class="text-lowercase">widget area description</p>
	</div>
</footer>
<!-- scripts inclusion from .ejs file -->
<% include partials/scripts %>
<!-- graphs inclusion from .ejs file  -->
<% include partials/graphs %>
<script src="/client/js/controllers/articles-client-controller.js"></script>
</body>
</html>